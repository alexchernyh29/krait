mixin header-lower()

    +b('section').header-lower
        .container 
            +e.wrap
                +e('ul').wrap__menu
                    +e('li').menu-text Каталог
                    +e('li').menu-text Акции
                    +e('li').menu-text Бренды
                    +e('li').menu-text Магазины
                    +e('li').menu-text О нас
                    +e('li').menu-text Доставка и оплата
                    +e('li').menu-text Статьи

